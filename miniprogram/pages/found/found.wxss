  padding: 0 30rpx;
}

.push{
  text-align: center;
  font-size: 25rpx;
  color: #C7C8CA;
}

.float-button{
  width: 130rpx;
  height: 130rpx;
  position: fixed;
  bottom:40rpx;
  right: 20rpx;
} 


.bg {
  position: absolute;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1001;
  -moz-opacity: 0.7;
  opacity: 0.70;
  filter: alpha(opacity=70);
}

.bg .share-text1{
  color: white;
  position: absolute;
  top: 30rpx;
  text-align: center;
  right: 40rpx;
}

.bg .share-text2{
  color: white;
  position: absolute;
  bottom: 130rpx;
  right: 40rpx;
  text-align: center;
}

.none-display{
  display: none;
}